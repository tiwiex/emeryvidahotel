
{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}

  {{ title_suffix }}
  {% block content %}
    <div{{ content_attributes.addClass('content') }}>
        <div class="sidebar-top second">
            <div class="sidebars">
                <div class="sidebar-content post-wrapper">
                  <div class="sidebarLabel">
                    <h3 {{ title_attributes }}>{{ label }}</h3>
                  </div>
                    {{ content }}
                </div>
            </div>
        </div>
    </div>
  {% endblock %}
</div>
