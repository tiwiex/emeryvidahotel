
<div class="news-lists"> 
    <div class="row">
        <div class="col-md-6 img">
            <div class="rooms-grid-1-wrapper">
                <img src="{{fields.field_image.content}}" alt="{{fields.field_image_1.content}}">
            </div>
        </div>     
        <div class="col-md-6">
            <div class="room-list">
                <div class="event-grid-1-wrapper">
                    <div class="newsEventDetail">
                        <div class="date-title">
                            <div class="event-date">
                                <p>{{fields.field_date.content}}</p>
                                <p>{{fields.field_date_1.content}}</p>
                            </div>
                            <div class="event-title">
                                <h3>{{fields.title.content}}</h3>
                            </div>
                        </div>    
                        <div class="discription">{{fields.body.content}}</p></div>
                        <div class="price-read">
                            <div class="roms-price">
                                {{fields.field_price.content}}
                                <p>{{fields.field_price_plan.content}}</p>
                            </div>
                            <div class="read">
                                <a href="{{fields.view_node.content}}" title="{{fields.view_node_1.content}}">{{fields.view_node_1.content}}</a>
                                <svg xmlns="http://www.w3.org/2000/svg" width="59" height="34.043" viewBox="0 0 59 34.043">
                                    <g id="arrow-right_1_" data-name="arrow-right (1)" transform="translate(2.5 3.521)">
                                        <path id="Path_28" data-name="Path 28" d="M0,0H52.786" transform="translate(0 13.5)" fill="none" stroke="#333239" stroke-linecap="round" stroke-width="5"/>
                                        <path id="Path_3" data-name="Path 3" d="M0,27,16.2,13.5,0,0" transform="translate(37.804 0)" fill="none" stroke="#333239" stroke-linecap="round" stroke-linejoin="round" stroke-width="5"/>
                                    </g>
                                </svg>
                            </div>
                        </div>    
                    </div>    
                </div>
            </div>
        </div>
    </div>    
</div>